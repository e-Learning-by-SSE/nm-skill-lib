        "@nx/eslint-plugin": "16.8.1",
        "@nx/jest": "16.8.1",
        "@nx/js": "16.8.1",
        "@nx/linter": "16.8.1",
        "@nx/node": "16.8.1",
        "@nx/workspace": "16.8.1",
        "@types/jest": "^29.4.0",
        "@types/node": "~18.7.1",
        "@typescript-eslint/eslint-plugin": "5.62.0",
        "@typescript-eslint/parser": "5.62.0",
        "esbuild": "0.19.2",
        "eslint": "8.46.0",
        "eslint-config-prettier": "8.1.0",
        "jest": "^29.6.2",
        "jest-environment-node": "^29.4.1",
        "jest-junit": "^16.0.0",
        "nx": "16.8.1",
        "prettier": "^2.6.2",
        "ts-jest": "^29.1.0",
        "ts-node": "10.9.1",
        "typescript": "~5.1.3"
    },
    "files": [
        "dist",
        "README.md",
        "LICENSE"
